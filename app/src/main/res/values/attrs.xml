<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 自定义加载进度-->
    <declare-styleable name="AsTimeDialogView">
        <!--左边圆的颜色-->
        <attr name="circle_left_color" format="color|reference"/>
        <!--右边圆的颜色-->
        <attr name="circle_right_color" format="color|reference"/>
        <!--圆的最大半径-->
        <attr name="circle_maxRadius" format="dimension|reference" />
        <!--左边圆的颜色-->
        <!--<attr name="circle_minRadius" format="dimension|reference" />-->
        <!--圆缩放一次的时间-->
        <attr name="circle_duration" format="integer" />
    </declare-styleable>

    <!--SwipeBack -->
    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension"/>
        <attr name="edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="shadow_left" format="reference"/>
        <attr name="shadow_right" format="reference"/>
        <attr name="shadow_bottom" format="reference"/>
    </declare-styleable>
    <attr name="SwipeBackLayoutStyle" format="reference"/>

    <!-- EditTextWithDrawable-->
    <declare-styleable name="EditTextWithDrawable">
        <attr name="drawableLeft" format="reference" />
        <attr name="drawableRight" format="reference" />
        <attr name="drawableRightEnable" format="reference" />
        <attr name="drawableLeftEnable" format="reference" />

    </declare-styleable>
</resources>